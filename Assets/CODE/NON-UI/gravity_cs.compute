            Grid0[cellID+TCC+1   ].jailerID, // up-right
            Grid0[cellID+1       ].jailerID, //right
            Grid0[cellID-TCC+1   ].jailerID, // down-right
            Grid0[cellID-TCC     ].jailerID, // down
            Grid0[cellID-TCC-1   ].jailerID, // down-left
            Grid0[cellID-2       ].jailerID, // 2left
            Grid0[cellID+TCC-2   ].jailerID, // up-2left
            Grid0[cellID+2*TCC-1 ].jailerID, // 2up-left
            Grid0[cellID+2*TCC   ].jailerID, // 2up
            Grid0[cellID+2*TCC+1 ].jailerID, // 2up-right
            Grid0[cellID+TCC+2   ].jailerID, // up-2right
            Grid0[cellID+2       ].jailerID, // 2right
            Grid0[cellID-TCC+2   ].jailerID, // down-2right
            Grid0[cellID-2*TCC+1 ].jailerID, // 2down-right
            Grid0[cellID-2*TCC   ].jailerID, // 2down
            Grid0[cellID-2*TCC-1 ].jailerID, // 2down-left
            Grid0[cellID-TCC-2   ].jailerID  // down-2left
        };
        for (int idx = 0; idx < ; ++id)
        {
            if (possibleID[id] != -1)
            {
                uint targetID = possibleID[id];
            
                //colision detectecion
                if (distance(currPoss, inputData[possibleID[id]].position) <= 1 && targetID != DispatchGroupIndex)
                {
                    currColo = float3(1,0,0);
                    
                    /*float2 jailerPoss = inputData[targetID].position;
