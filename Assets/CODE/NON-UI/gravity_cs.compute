[numthreads(1024, 1, 1)]
void WipeGrid0(uint3 id : SV_DispatchThreadID)
{
    if (id.x >= GridTotalCellCount[0].x) { return; }
    
    Grid0[id.x].mass = 0;
    Grid0[id.x].MassTPossition = 0;
    Grid0[id.x].position = float2(GridTargetPosition.x, GridTargetPosition.y);
    Grid0[id.x].jailerID = -1;
